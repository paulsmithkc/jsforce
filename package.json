{
  "author": "Paul Smith",
  "name": "jsforce-micro",
  "version": "0.0.1",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com",
    "jsforce",
    "micro",
    "microlibrary"
  ],
  "homepage": "https://github.com/paulsmithkc/jsforce/blob/jsforce-micro/README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/paulsmithkc/jsforce.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/paulsmithkc/jsforce/raw/jsforce-micro/LICENSE"
    }
  ],
  "main": "./dist/jsforce.js",
  "scripts": {
    "reinstall": "npx rimraf --glob package-lock.json node_modules && npm i",
    "build": "npx tsup",
    "doc": "jsdoc lib -d doc --recurse --lenient",
    "test": "npm run test:node",
    "test:all": "npm run test:node && npm run test:browser",
    "test:node": "npx mocha --retries 2 --reporter spec --require intelli-espower-loader --exit",
    "test:browser": "testem",
    "test:setup": "node -r dotenv/config ./test/bin/org-setup"
  },
  "directories": {
    "test": "test/"
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "lib",
    "core.js",
    "index.js"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "dependencies": {
    "csv-parse": "^4.10.1",
    "csv-stringify": "^1.0.4",
    "faye": "^1.4.0",
    "inherits": "^2.0.1",
    "lodash": "^4.17.19",
    "multistream": "^2.0.5",
    "promise": "^7.1.1",
    "readable-stream": "^2.1.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.13",
    "@types/request": "^2.48.8",
    "archiver": "^1.3.0",
    "async": "^1.5.2",
    "bufferutil": "^4.0.0",
    "dotenv": "^16.0.0",
    "envify": "^3.4.1",
    "espower-loader": "^1.2.2",
    "espowerify": "^1.1.0",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.6.3",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.0",
    "puppeteer": "^1.18.1",
    "require-swapper": "^0.1.7",
    "testem": "^2.8.2",
    "through2": "^2.0.3",
    "tsup": "^6.7.0",
    "utf-8-validate": "^5.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "browser": {
    "./index": "./lib/browser/jsforce",
    "./index.js": "./lib/browser/jsforce",
    "./core": "./lib/browser/core",
    "./core.js": "./lib/browser/core",
    "request": "./lib/browser/request.js"
  }
}
