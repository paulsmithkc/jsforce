{
  "compilerOptions": {
    "incremental": false,
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "rootDirs": ["./lib"],
    "outDir": "dist",
  },
  "exclude": ["./node_modules", "./dist"]
}